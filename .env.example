# JWT
JWT_SECRET=your_super_secret_jwt_key_here_make_it_long_and_secure
JWT_EXPIRATION=24h
REFRESH_EXPIRATION=720h

# App
APP_NAME=fluently
APP_HOST=0.0.0.0
APP_PORT=8070

# Database
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=postgres
DB_PORT=5432
DB_NAME=postgres
DB_DATABASE=postgres

# Directus
DIRECTUS_PORT=8055
DIRECTUS_ADMIN_EMAIL=example@example.com
DIRECTUS_ADMIN_PASSWORD=your_super_secure_password_here
DIRECTUS_SECRET_KEY=fluently-instance-1

# Google OAuth
IOS_GOOGLE_CLIENT_ID=example-ios-client-id
ANDROID_GOOGLE_CLIENT_ID=example-android-client-id
WEB_GOOGLE_CLIENT_ID=example-web-client-id
WEB_GOOGLE_CLIENT_SECRET=example-web-client-secret

# Swagger
SWAGGER_ALLOWED_EMAILS=example@example.com

# Logging Configuration
LOG_LEVEL=info
LOG_PATH=./logs/app.log

# Security Configuration
PASSWORD_MIN_LENGTH=8
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_DURATION=1h

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=your_super_secure_password_here

# SonarQube Configuration
SONAR_JDBC_USERNAME=${DB_USER}
SONAR_JDBC_PASSWORD=${DB_PASSWORD}

# ZeroTier Configuration
ZEROTIER_IP=192.168.0.1

# Public URLs
PUBLIC_URL=https://fluently-app.ru
SWAGGER_HOST=192.168.0.1:8070
SWAGGER_HOST=192.168.0.1:8070
BOT_TOKEN=your_bot_token_here
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_URL=https://fluently-app.ru/webhook
REDIS_ADDR=redis:6379
GROQ_API_KEYS=gsk_ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890
GEMINI_API_KEYS=ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890